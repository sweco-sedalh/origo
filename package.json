{
  "name": "origo",
  "version": "2.9.1-dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/origo-map/origo.git"
  },
  "description": "Framework to build web map apps",
  "keywords": [
    "map",
    "mapping",
    "openlayers",
    "origo"
  ],
  "license": "BSD-2-Clause",
  "main": "./dist/origo.umd.cjs",
  "module": "./dist/origo.js",
  "exports": {
    ".": {
      "import": "./dist/origo.js",
      "require": "./dist/origo.umd.cjs"
    },
    "./origo.css": "./dist/origo.css"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve",
    "lint": "eslint -c .eslintrc.json origo.ts src/",
    "lint-fix": "eslint -c .eslintrc.json origo.ts src/ --fix",
    "build": "vite build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@glidejs/glide": "^3.6.2",
    "awesomplete": "^1.1.7",
    "cuid": "^2.1.8",
    "downloadjs": "^1.4.7",
    "drag-drop-touch": "^1.3.1",
    "elm-pep": "^1.0.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "ol": "^10.2.1",
    "ol-mapbox-style": "12.3.5",
    "pepjs": "^0.5.3",
    "proj4": "^2.12.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/proj4": "^2.5.6",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "run-sequence": "^2.2.1",
    "sass-embedded": "^1.82.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.6"
  }
}
